{
  "expo": {
    "name": "Fallio",
    "slug": "Fallio - Gerçek Fal Uygulaması",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/Fallio_ICON.png",
    "scheme": "fallio",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Fallio_ICON.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.moiv.fallio",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Fal göndermek için fotoğraf galerisine erişim izni gereklidir.",
        "NSCameraUsageDescription": "Fal fotoğrafı çekmek için kamera erişim izni gereklidir.",
        "NSMicrophoneUsageDescription": "Uygulama içi sesli özellikler için mikrofon erişimi gereklidir.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Fallio_ICON.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.moiv.fallio",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/Fallio_ICON.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://fallio-web.vercel.app/"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-5812875423104468~8803913008",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "fallio-mobile",
          "organization": "moiv-interactive"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://fallio-web.vercel.app/"
      },
      "eas": {
        "projectId": "5711fe50-bf31-4f30-87ce-ac1811dc9836"
      }
    },
    "owner": "velcrinn"
  }
}